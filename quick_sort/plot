reset  
set title 'my plot'
set xlabel 'number of element'
set ylabel 'time(sec)'
set style fill solid
set title 'performance comparison between random data and ascending data'
set terminal png
set output 'runtime.png'

plot "out_random.txt" using 1 with points title "random data" linecolor "orange", \
     "out_ascending.txt" using 1 with points title "ascend data", \
     "out_descend.txt" using 1 with points title "descend data" linecolor "red", \
     "out_r.txt" using 1 with points title "random data with random pivot" linecolor "blue", \
     "out_ra.txt" using 1 with points title "ascend data with random pivot" linecolor "pink", \
     "out_rd.txt" using 1 with points title "descend data with random pivot" linecolor "purple"
replot 
